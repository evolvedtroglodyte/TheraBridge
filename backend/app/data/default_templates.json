[
  {
    "id": "f7e8a1b2-c3d4-4e5f-9a8b-1c2d3e4f5a6b",
    "name": "SOAP Note",
    "description": "Standard SOAP format for medical and therapy documentation",
    "template_type": "soap",
    "is_system": true,
    "is_shared": false,
    "created_by": null,
    "structure": {
      "sections": [
        {
          "id": "subjective",
          "name": "Subjective",
          "description": "Patient's reported experience and concerns",
          "fields": [
            {
              "id": "chief_complaint",
              "label": "Chief Complaint",
              "type": "textarea",
              "required": true,
              "ai_mapping": "key_topics, topic_summary"
            },
            {
              "id": "history",
              "label": "History of Present Illness",
              "type": "textarea",
              "required": false,
              "ai_mapping": "context, background"
            },
            {
              "id": "mood",
              "label": "Reported Mood",
              "type": "text",
              "required": false,
              "ai_mapping": "session_mood"
            }
          ]
        },
        {
          "id": "objective",
          "name": "Objective",
          "description": "Clinician's observations during session",
          "fields": [
            {
              "id": "appearance",
              "label": "Appearance",
              "type": "text",
              "required": false,
              "ai_mapping": "presentation"
            },
            {
              "id": "behavior",
              "label": "Behavior",
              "type": "text",
              "required": false,
              "ai_mapping": "engagement, presentation"
            },
            {
              "id": "affect",
              "label": "Affect",
              "type": "select",
              "required": false,
              "options": [
                "appropriate",
                "flat",
                "labile",
                "constricted"
              ],
              "ai_mapping": "emotional_state"
            }
          ]
        },
        {
          "id": "assessment",
          "name": "Assessment",
          "description": "Clinical interpretation and evaluation",
          "fields": [
            {
              "id": "clinical_impression",
              "label": "Clinical Impression",
              "type": "textarea",
              "required": true,
              "ai_mapping": "clinical_insights, progress"
            },
            {
              "id": "risk_assessment",
              "label": "Risk Assessment",
              "type": "textarea",
              "required": false,
              "ai_mapping": "risk_flags, safety_concerns"
            }
          ]
        },
        {
          "id": "plan",
          "name": "Plan",
          "description": "Treatment plan and next steps",
          "fields": [
            {
              "id": "interventions",
              "label": "Interventions Used",
              "type": "textarea",
              "required": false,
              "ai_mapping": "strategies, techniques"
            },
            {
              "id": "homework",
              "label": "Homework/Action Items",
              "type": "textarea",
              "required": false,
              "ai_mapping": "action_items"
            },
            {
              "id": "next_session",
              "label": "Next Session Plan",
              "type": "textarea",
              "required": false,
              "ai_mapping": "next_steps"
            }
          ]
        }
      ]
    }
  },
